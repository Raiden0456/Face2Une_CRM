    "schemes": ["http"],
    "consumes": ["application/json"],
    "produces": ["application/json"],
    "paths": {
        "/appointments": {
          "get": {
            "tags": ["Procedures"],
            "summary": "Get all procedures in system",
            "responses": {
              "200": {
                "description": "OK",
                "schema": {
                  "$ref": "postgres/public/procedures"
                }
              }
            }
          }
        }
      },
      "definitions": {
        "Procedure": {
          "required": ["name", "_id", "additional"],
          "properties": {
            "_id": {
              "type": "integer",
              "uniqueItems": true
            },
            "name": {
              "type": "string"
            },
            "description": {
              "type": "string"
            },
            "price": {
              "type": "number"
            },
            "duration": {
              "type": "number"
            },
            "additional": {
              "type": "number"
            }
          }
        }
      }
  }